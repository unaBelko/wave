<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.armin.wave">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.armin.wave.home.HomeActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.example.armin.wave.register.RegisterActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.example.armin.wave.utakmice.UtakmiceActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.example.armin.wave.standings.StandingsActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.example.armin.wave.daily_results.DailyResultsActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.example.armin.wave.matchBet.MatchBetActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.example.armin.wave.ticket.TicketActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.example.armin.wave.TicketHistory.TicketHistory"
            android:label="@string/app_name" />
        <activity
            android:name="com.example.armin.wave.user.UserActivity"
            android:label="@string/app_name" />
    </application>

</manifest>